script(type='text/javascript').
	$("#dropdown-in-player li a").click(function(){
		$(this).parents(".dropdown").find('.btn').html($(this).text() + '<i class="fa fa-caret-down"></i>');
		$(this).parents(".dropdown").find('.btn').val($(this).data('value'));

		$('#top5-player-content .top-level-panel').hide('tab');
		$($(this).attr('value')).show('tab');
	});
	$(".modal-content .modal-body a").click(function(){
		$('#top5-player-content .top-level-panel').hide('tab');
		$($(this).attr('value')).show('tab');
		$("h5.text-center.visible-xxs.subtitle.position span").html($(this).text());
		$('#playerModal').modal('hide');
	});
	$("ul#dropdown-in-player").hover(function(){
		$("button.btn.btn-primary").css( "box-shadow", "none" );
	});
